{
	"name": "conal",
	"version": "0.0.0",
	"packageManager": "pnpm@10.11.0",
	"description": "Classic FRP library for TypeScript",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/bglgwyng/conal#readme",
	"bugs": {
		"url": "https://github.com/bglgwyng/conal/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bglgwyng/conal.git"
	},
	"author": "bgl gwyng",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint": "biome check .",
		"lint:fix": "biome check --fix .",
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"release": "bumpp && pnpm publish",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.2",
		"@types/assert": "^1.5.11",
		"@types/node": "^22.15.17",
		"bumpp": "^10.1.0",
		"happy-dom": "^17.4.7",
		"tsdown": "^0.11.9",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	},
	"dependencies": {
		"assert": "^2.1.0"
	}
}